        <script><![CDATA[var rightNow = new GlideDateTime();
var twentyfourfromNow = gs.hoursAgo(-24);

var needItTaskGR = new GlideRecord('x_58872_needit_needit_task');
needItTaskGR.addQuery('due_date', '>=', rightNow);
needItTaskGR.addQuery('due_date', '<=', twentyfourfromNow);
needItTaskGR.addQuery('state', '!=', 3);
needItTaskGR.query();

while(needItTaskGR.next()){
	gs.info('Record with due date between now and 24 hours from now: ' + needItTaskGR.number);
	gs.eventQueue('x_58872_needit.dueSoonNeedItTasks', needItTaskGR, needItTaskGR.number, gs.getUserName());
}

]]></script>
